						"value": "[parameters('HTTP_solliancerepublicdata_Customer_properties_typeProperties_url')]"
					},
					"sql_haniffajri_properties_typeProperties_server": {
						"value": "[parameters('sql_haniffajri_properties_typeProperties_server')]"
					},
					"sql_haniffajri_properties_typeProperties_database": {
						"value": "[parameters('sql_haniffajri_properties_typeProperties_database')]"
					},
					"sql_haniffajri_properties_typeProperties_userName": {
						"value": "[parameters('sql_haniffajri_properties_typeProperties_userName')]"
					},
					"sql_rizalmuja_properties_typeProperties_server": {
						"value": "[parameters('sql_rizalmuja_properties_typeProperties_server')]"
					},
					"sql_rizalmuja_properties_typeProperties_database": {
						"value": "[parameters('sql_rizalmuja_properties_typeProperties_database')]"
					},
					"sql_rizalmuja_properties_typeProperties_userName": {
						"value": "[parameters('sql_rizalmuja_properties_typeProperties_userName')]"
					},
					"SQL_rapidtech_properties_typeProperties_server": {
						"value": "[parameters('SQL_rapidtech_properties_typeProperties_server')]"
					},
					"SQL_rapidtech_properties_typeProperties_database": {
						"value": "[parameters('SQL_rapidtech_properties_typeProperties_database')]"
					},
					"SQL_rapidtech_properties_typeProperties_userName": {
						"value": "[parameters('SQL_rapidtech_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfrizalmuja_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfrizalmuja_ArmTemplate_0')]"
			]
		}
	]
}